///genre_return(genre)
#args theme

switch(theme) {
  case genres.action:
   return "Action"
   break;
  case genres.RPG:
   return "RPG"
   break;
  case genres.strategy:
   return "Strategy"
   break;
  case genres.sim:
   return "Simulator"
   break;
  case themes.adventure:
   return "Adventure"
   break;
}
