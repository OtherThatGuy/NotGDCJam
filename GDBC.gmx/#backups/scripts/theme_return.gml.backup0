///theme_return(theme)
#args theme

switch(theme) {
  case themes.sci_fi:
   return "Sci-fi"
   break;
  case themes.fantasy:
   return "Fantasy"
   break;
  case themes.comedy:
   return "Comedy"
   break;
  case themes.horror:
   return "Horror"
   break;
  case themes.mystery:
   return "Mystery"
   break;
}
