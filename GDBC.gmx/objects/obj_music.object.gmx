            <string>//timers
if (timer != 0) {
    timer = max(0,timer-1);
} else {
    done = true;    
}

//note shit
if (!done) {
if (note_spawn_timer = 0) {
    var rand = irandom_range(0,7);
    
    if (Chance(0.60)) {
     var cur_note = instance_create(room_width+64,lanes[rand],obj_notes);
     cur_note.note_id = notes[current_note];
    } else {
     instance_create(room_width+64, lanes[rand], obj_notes);
    }
    note_spawn_timer = max_spawn_timer;
} else if (note_spawn_timer != -1) {
    note_spawn_timer -= 1;
    note_spawn_timer = max(0, note_spawn_timer);
    
}


//schut timer
if (ship_shoot_timer != 0) {
    ship_shoot_timer -= 1;
    ship_shoot_timer = max(0,ship_shoot_timer);
}

//note timer

//ship controls
if (keyboard_check_pressed(vk_up)) {
    
   if (cur_lane != 0) {
       cur_lane -= 1;
   } else {
       exit;
   }
   
   ship_y = lanes[cur_lane];
    
}

if (keyboard_check_pressed(vk_down)) {
    
   if (cur_lane != 7) {
       cur_lane += 1;
   } else {
       exit;
   }
   
   ship_y = lanes[cur_lane];
    
}

if (keyboard_check_pressed(vk_space) and ship_shoot_timer = 0) {
    var schut = instance_create(ship_x+32,ship_y+ship_shot_offset,obj_bullet);
    schut.direction = 0;
    schut.speed = 25;
    
    ship_shoot_timer = max_shoot_timer;
    
}

if (current_note &gt;= 8) {
    
    pick_jingle(true);
    
}

}

if (done) {
 if (gave_points == false) {
  global.music_points += m_score * 1000;
  global.days_left -= 1;
  gave_points = true;
 } 
 
 accept = collision_button(640, name_box_y-64, 128, 36);
 
 if (accept and mouse_check_button_pressed(mb_left)) {
  scr_changeRoom(roomMain);
 }
 
}


